(* untyped church numeral, very classical  *)

let 0 = λf x.x;
let S = λn f x.f (n f x);
let 1 = S 0;
let 2 = S 1;
let 3 = S 2;
let 4 = S 3;
let 5 = S 4;
let 6 = S 5;
let 7 = S 6;
let 8 = S 7;
let 9 = S 8;
let 10 = S 9;
let add = λn m f x.n f (m f x);
let mul = λn m f.n (m f);
let 20 = add 10 10;
let 30 = add 20 10;
let 40 = add 30 10;
let 100 = mul 10 10;

let pred = λn.n
	(λp x y.p (S x) x)
	(λx y.y) 0 0;
